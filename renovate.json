{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard"
  ],
  "labels": [
    "dependencies"
  ],
  "schedule": [
    "every weekend"
  ],
  "prHourlyLimit": 5,
  "prConcurrentLimit": 10,
  "ignoreDeps": [],
  "ignorePaths": [],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["^config/nvim/lazy-lock\\.json$"],
      "matchStrings": [
        "\"(?<depName>.*?)\": \\{ \"branch\": \"(?<branchName>.*?)\", \"commit\": \"(?<currentDigest>.*?)\" \\}"
      ],
      "datasourceTemplate": "github",
      "versioningTemplate": "git",
      "currentValueTemplate": "{{branchName}}"
    }
  ]
}
