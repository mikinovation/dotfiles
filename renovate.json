{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":dependencyDashboard",
  ],
  "labels": [
    "dependencies"
  ],
  "schedule": [
    "every weekend"
  ],
  "prHourlyLimit": 5,
  "prConcurrentLimit": 10,
  "rangeStrategy": "pin",
  "regexManagers": [
    {
      "fileMatch": [".*\\.lua$"],
      "matchStrings": [
        "\\s*['\\\"](?<depName>[^'\\\"]+)['\\\"],\\s*version\\s*=\\s*['\\\"](?<currentValue>[^'\\\"]+)['\\\"]"
      ],
      "datasourceTemplate": "github-tags"
    },
    {
      "fileMatch": [".*\\.lua$"],
      "matchStrings": [
        "\\s*['\\\"](?<depName>[^'\\\"]+)['\\\"],\\s*commit\\s*=\\s*['\\\"](?<currentDigest>[^'\\\"]+)['\\\"]"
      ],
      "datasourceTemplate": "github-commits"
    }
  ],
  "ignoreDeps": [],
  "ignorePaths": [],
}
