{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
    ':dependencyDashboard',
  ],
  labels: [
    'dependencies',
  ],
  schedule: [
    'before 5am',
  ],
  customManagers: [
    {
      "customType": "jsonata",
      "fileFormat": "json",
      "fileMatch": ["lazy-lock.json$"],
      "matchStrings": [
        "$each($, function($v, $k) { {'depName': $k, 'currentValue': $v.commit, 'datasource': 'git-refs', 'packageName': 'https://github.com/' & $k & '.git'} })"
      ]
    }
  ],
}
