{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
    ':dependencyDashboard',
  ],
  labels: [
    'dependencies',
  ],
  schedule: [
    'every weekend',
  ],
  prHourlyLimit: 5,
  prConcurrentLimit: 10,
  ignoreDeps: [],
  ignorePaths: [],
  customManagers: [
    {
      customType: 'regex',
      fileMatch: [
        '^config/nvim/plugins/.*\\.lua$',
      ],
      matchStrings: [
        '"(?<depName>[^"]+/[^"]+)",\\s*commit\\s*=\\s*"(?<currentDigest>[a-f0-9]+)"',
      ],
      datasourceTemplate: 'github',
      versioningTemplate: 'git',
    },
  ],
}
