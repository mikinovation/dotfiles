{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
    ':dependencyDashboard',
  ],
  labels: [
    'dependencies',
  ],
  schedule: [
    'every weekend',
  ],
  prHourlyLimit: 5,
  prConcurrentLimit: 10,
  ignoreDeps: [],
  ignorePaths: [],
  packageRules: [
    {
      matchStrings: [
        'renovate: datasource=(?<datasource>.*?) depName=(?<depName>.*?)\\n',
      ],
      extractVersion: '^\\s*commit\\s*=\\s*["\']?(?<version>[a-f0-9]+)["\']?\\s*,?\\n',
      matchPackageNames: [
        '/^config/nvim/plugins/.*\\.lua$/',
      ],
    },
  ],
}
